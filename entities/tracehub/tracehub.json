{
  "version": "2025-03-17.01",
  "entity": "TraceHub",
  "role": "tracing_module",
  "status": "stateless",
  "description": "TraceHub coordinates correlated tracing across the Temporal Loom with governed audit storage and export controls.",
  "manifests": {
    "library": "aci://entities/tracehub/library/tracehub_library.json",
    "memory": "aci://entities/tracehub/memory/tracehub_memory.json",
    "modules": "aci://entities/tracehub/modules/tracehub_modules.json",
    "playbook": "aci://entities/tracehub/memory/tracehub_playbook.json"
  },
  "logging": {
    "export_path": "/mnt/tracehub",
    "notes": "All TraceHub output is streamed to a temporary /mnt/tracehub mount and surfaced via the process_logs wrapper.",
    "process_logs_wrapper": "aci://library/wrappers/process_logs/process_logs.json",
    "router_manifest": "aci://library/wrappers/tracehub/tracehub_router.json",
    "status_wrapper": "aci://library/wrappers/tracehub/tracehub_status.json"
  },
  "storage": {
    "router_key_root": "audit_router.tracehub",
    "routes": {
      "session": "audit_router.tracehub.session",
      "export": "audit_router.tracehub.export"
    },
    "session_path": "/mnt/tracehub/session",
    "export_path": "/mnt/tracehub/export",
    "default_mode": "session",
    "toggle_flags": [
      "audit_router.tracehub.export"
    ],
    "export_requires": [
      "TVA.ok",
      "Sentinel.ok"
    ]
  }
}
